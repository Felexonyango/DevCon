{
  "name": "devconnector",
  "version": "1.0.0",
  "engines": {
    "node": "13.12.0",
    "npm": "6.14.4"
  },
  "description": "Social network for developers",
  "main": "server.js",
  "scripts": {
    "start": "node  server",
    "server": "nodemon  server",
    "test": "mocha --timeout 50000 --exit",
