    "axios": "^0.24.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.1",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "config": "^3.3.6",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "express-validator": "^6.4.0",
    "gravatar": "^1.8.0",
    "http-proxy-middleware": "^2.0.6",
